CREATE TABLE Loot( 

    [LootID] INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, 

    [LootName] NVARCHAR(40) NOT NULL, 

    [LootType] NVARCHAR(40) 

); 

  

CREATE TABLE Enemy( 

    [EnemyID] INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, 

    [EnemyName] NVARCHAR(80) NOT NULL, 

    [EnemyHP] INTEGER, 

    [EnemySouls] INTEGER, 

    [EnemyWeakness] NVARCHAR(40), 

    [EnemyResistance] NVARCHAR(40), 

    [EnemyImmunity] NVARCHAR(40), 

    [EnemyBoss] NVARCHAR(10) NOT NULL 

); 

  

CREATE TABLE Location( 

    [LocationID] INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, 

    [LocationName] NVARCHAR(80) NOT NULL, 

    [LocationBonfires] INTEGER, 

    [LocationRecLevel] INTEGER, 

    [LocationHasBoss] NVARCHAR(10) NOT NULL 

); 

  

CREATE TABLE Drops( 

    [EnemyID] INTEGER NOT NULL, 

    [LootID] INTEGER NOT NULL, 

    CONSTRAINT fk_enemy_drops FOREIGN KEY (EnemyID) REFERENCES Enemy(EnemyID), 

    CONSTRAINT fk_loot_drops FOREIGN KEY (LootID) REFERENCES Loot(LootID) 

); 

  

CREATE TABLE Patrols( 

    [EnemyID] INTEGER NOT NULL, 

    [LocationID] INTEGER NOT NULL, 

    CONSTRAINT fk_enemy_patrols FOREIGN KEY (EnemyID) REFERENCES Enemy(EnemyID), 

    CONSTRAINT fk_location_patrols FOREIGN KEY (LocationID) REFERENCES Location(LocationID) 

); 

 

